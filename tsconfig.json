{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "types": ["react", "react-native"],
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}

